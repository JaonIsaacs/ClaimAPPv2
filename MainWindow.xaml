<Window x:Class="ClaimAPPv2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClaimAPPv2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame Name="MainFrame" NavigationUIVisibility="Hidden" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Bottom">
            <Button Content="Submit Claim" Click="SubmitClaim_Click" />
            <Button Content="View Claims" Click="ViewClaims_Click" />
            <Button Content="Approve/Deny Claim" Click="ApproveDenyClaim_Click" />
        </StackPanel>
    </Grid>
</Window>
